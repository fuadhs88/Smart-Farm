
# Generated by FastPlaz

RewriteEngine On
AddHandler cgi-script .cgi
AddHandler cgi-script .bin
AddHandler cgi-script .pas
AddHandler cgi-script .exe
Options +ExecCGI

DirectoryIndex index.html
ErrorDocument 500 '{"code": 500}'
ErrorDocument 404 '{"code": 404}'

# Cross-Origin
Header always set Access-Control-Allow-Origin "*"

SetEnvIf Authorization "(.*)" Authorization=$1
SetEnvIf ClientID "(.*)" ClientID=$1
SetEnvIf ClientSecret "(.*)" ClientSecret=$1
SetEnvIf Token "(.*)" Token=$1

SetEnvIf Authorization "(.*)" Authorization=$1

RewriteRule ^(stations|station)/(.*)$ station.bin/$2 [QSA,L]
RewriteRule ^(nodes|node)/(.*)$ node.bin/$2 [QSA,L]
RewriteRule ^(schedules|schedule)/(.*)$ schedule.bin/$2 [QSA,L]
RewriteRule ^(issues|issue)/(.*)$ issue.bin/$2 [QSA,L]
RewriteRule ^(stats)/(.*)$ stats.bin/$2 [QSA,L]
RewriteRule ^(auth)/(.*)$ auth.bin/$2 [QSA,L]


RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f [OR]
RewriteCond %{REQUEST_FILENAME} -l
RewriteRule ^(.*)$ - [NC,L]
#RewriteRule ^(.*)$ 404.php [QSA,L]
#RewriteRule ^(.*)$ index.html?$1 [QSA,L]
